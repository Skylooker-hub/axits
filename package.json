{
  "name": "@huang1997/axits",
  "version": "0.1.6",
  "private": false,
  "description": "",
  "main": "build/axits.cjs.js",
  "module": "build/axits.es.js",
  "types": "build/axits.d.ts",
  "scripts": {
    "prepare": "husky install",
    "cm": "git cz",
    "dev": "rollup -w -c rollup.config.ts --environment MODE:dev",
    "build": "rollup -c rollup.config.ts --environment MODE:prod",
    "genlog": "standard-version",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "release": "node scripts/release.js",
    "test": "jest --coverage=false",
    "test:coverage": "jest"
  },
  "lint-staged": {
    "lib/**/*.{ts,js}": [
      "prettier --write",
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e -V"
    }
  },
  "files": [
    "build"
  ],
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skylooker-hub/axits.git"
  },
  "keywords": [
    "axios",
    "typescript",
    "axits"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Skylooker-hub/axits/issues"
  },
  "homepage": "https://github.com/Skylooker-hub/axits#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@microsoft/api-extractor": "^7.19.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "conventional-changelog-cli": "^2.2.2",
    "cz-customizable": "^6.3.0",
    "enquirer": "^2.3.6",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "execa": "^5.1.1",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "minimist": "^1.2.5",
    "prettier": "2.5.1",
    "rollup": "^2.63.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "semver": "^7.3.5",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "axios": "^0.26.1"
  }
}
